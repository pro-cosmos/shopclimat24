{% if on_category %}
<div class="options{% if get_category_id %} filter_category_page{% endif %}" id="home_filter">
<div class="margin-bottom">
<div class="filter">
<form action="index.php?" method="get" id="filter_send">
<input type="hidden" name="route" value="product/category" />
{% if page_category and page_category != "" or category_id %}
<input type="hidden" name="path" value="{{ category_id }}" />
<input type="hidden" name="category_id" value="{{ category_id }}" class="categ" />
{% endif %}
{% if manufacturer_id %}
<input type="hidden" name="manufacturer_id" value="{{ manufacturer_id }}" />
{% endif %}
{% if search %}
<input type="hidden" name="search" value="{{ search }}" />
{% endif %}
{% if parent_category_hierarchy.category_id %}
<input type="hidden" name="parent_category_hierarchy" value="{{ parent_category_hierarchy.category_id }}" checked id="parent_category_hierarchy" />
<input type="hidden" name="parent_category_hierarchy_name" value="{{ parent_category_hierarchy.name_hierarchy }}" checked id="parent_category_hierarchy_name" />
{% endif %}
{% set popup_on = true %}
{% if keywords_filter_text and keywords_filter_text != "" %}<input type="hidden" name="keywords_filter" value="{{ keywords_filter_text }}" />{% endif %}
{% if max_price_const or old_price %}<input type="hidden" name="op" value="{% if max_price_const %}{{ max_price_const }}{% else %}{{ old_price }}{% endif %}" class="filter_op" />{% endif %}
{% set drop_option = true %}{% set drop_attributes = true %}
{% if popup_view %}{% set popup_on = false %}{% endif %}
{% if max_height == false %}{% set max_height = '215' %}{% endif %}
{% if count_show == false %}{% set count_show = 3 %}{% endif %}
{% if status_seo %}{% set seo_on = status_seo %}{% else %}{% set seo_on = false %}{% endif %}
{% if seo_separator == false %}{% set seo_separator = "=" %}{% endif %}
<div id="option_filter">
	{% if name_module and name_module != "" %}<h3 class="col-sm-12"><i class="fa fa-filter"></i>&nbsp;&nbsp;&nbsp;{{ name_module }}</h3>{% endif %}
	<div class="clearfix">
	<div class="options_container_filter col-sm-12">
	<div class="clearfix"></div>
	<div class="options_container filter_container">
	{% if category_id %}<a class="btn btn-white category_filt" onclick="{% if post_category_id == false %}{% if category_type=="radio" or category_type=="link" %}live_option_product('category_filter', 'options', '', '');{% else %}live_home_clear_category_filter({{ category_id }}){% endif %}{% else %}live_home_clear_category_filter({{ old_category_id }}){% endif %}"><span class="pull-left">{{ category_name }}</span>&nbsp;&nbsp;<i class="fa fa-times pull-right" aria-hidden="true"></i></a>{% endif %}
		{% if get_price_max and max_price_const %}
			{% if get_price_max > max_price_const %}
				{% if max_price_const %}{% set test_price = max_price_const %}{% endif %}
			{% else %}
				{% if get_price_max %}{% set test_price = get_price_max %}{% endif %}
			{% endif %}
				{% if test_price != max_price_const or get_price_min != 0 %}
				{% for currency in currencies %}
					{% if currency.symbol_left and currency.code == code %}
						{% set value_max_currency = currency.symbol_left %}
					{% elseif currency.symbol_right and currency.code == code %}
						{% set value_max_currency = currency.symbol_right %}
					{% endif %}
				{% endfor %}
				<a class="btn btn-white option_cloud_price" onclick="slider_generation('0','{{ max_price_const }}','clear'); clear_popup_filter();"><span class="pull-left">{{ text_price }} {% if min_price %}{{ min_price }}{% endif %} - {% if get_price_max > max_price_const %}{% if max_price_const %}{{ max_price_const }}{% endif %}{% else %}{% if max_price %}{{ max_price }}{% endif %}{% endif %}{% if value_max_currency %}{{ value_max_currency }}{% endif %}</span>&nbsp;&nbsp;<i class="fa fa-times pull-right" aria-hidden="true"></i></a>
			{% endif %}
		{% endif %}
		{% if manufacturers %}
			{% set n = 2000 %}
			{% for manufacturer in manufacturers %}
				{% if manufacturer.manufacturer_value_required %}
					<a class="btn btn-white option_cloud{{ n }}{{ manufacturer.manufacturer_id }}" onclick="{% if manufacturers_type == 'radio' %}live_option_product('manufacturers_filter', 'options', '', '');{% else %}live_home_clear_manufacturers_filter({{ manufacturer.manufacturer_id }});{% endif %}"><span class="pull-left">{{ manufacturer.name }}</span>&nbsp;&nbsp;<i class="fa fa-times pull-right" aria-hidden="true"></i></a>
				{% endif %}
				{% set n=n+1 %}
			{% endfor %}
		{% endif %}
		{% set k=1 %}
		{% if keywords_filter %}
			{% for value in keywords_filter %}
				{% if value != "" %}
					<a class="btn btn-white option_cloud_keywords{{ k }}" onclick="live_home_clear_keywords_filter('{{ value }}', {{ k }});" data-value="{{ value }}"><span class="pull-left">{{ value }}</span>&nbsp;&nbsp;<i class="fa fa-times pull-right" aria-hidden="true"></i></a>
				{% endif %}
				{% set k=k+1 %}
			{% endfor %}
		{% endif %}
		{% if stock_statuses %}
			{% for stock_status in stock_statuses %}
				{% if stock_status.stock_status_value_required %}
					<a class="btn btn-white option_cloud{{ stock_status.status_id }}" onclick="{% if status_stock_type == 'radio' %}live_option_product('stock_status_filter', 'options', '', '');{% else %}live_home_clear_stock_filter('{{ stock_status.status_id }}');{% endif %}"><span class="pull-left">{{ stock_status.name }}</span>&nbsp;&nbsp;<i class="fa fa-times pull-right" aria-hidden="true"></i></a>
				{% endif %}
			{% endfor %}
		{% endif %}
		{% if options %}
			{% for option in options %}
				{% for option_value in option.option_value %}
					{% if option_value.option_value_required %}
						<a class="btn btn-white option_cloud{{ option_value.option_value_id }}" onclick="live_home_clear_option_filter({{ option_value.option_value_id }});"><span class="pull-left">{{ option.name }} : {{ option_value.option_value_name }}</span>&nbsp;&nbsp;<i class="fa fa-times pull-right" aria-hidden="true"></i></a>
					{% endif %}
				{% endfor %}
			{% endfor %}
		{% endif %}
		{% if filters %}
			{% for filter in filters %}
				{% for filter_value in filters.filter %}
					{% if filter_value.filter_value_required %}
						<a class="btn btn-white option_cloud{{ filter_value.filter_id }}" onclick="live_home_clear_filter_filter({{ filter_value.filter_id }});"><span class="pull-left">{{ filter.name }} : {{ filter_value.name }}</span>&nbsp;&nbsp;<i class="fa fa-times pull-right" aria-hidden="true"></i></a>
					{% endif %}
				{% endfor %}
			{% endfor %}
		{% endif %}
		{% if attributes %}
			{% set m = 1000 %}
			{% for attribute in attributes %}
                {% if attribute.attribute_show_required %}
    				{% for attribute_text in attribute.attribute %}
                        {% if attribute_text.attribute_value_required and attribute_text.attribute_stock_required %}
    						<a class="btn btn-white option_cloud{{ m }}{{ attribute.attribute_id }}" onclick="live_home_clear_attributes_filter({{ m }}{{ attribute.attribute_id }});"><span class="pull-left">{{ attribute.name }} : {{ attribute_text.text }}</span>&nbsp;&nbsp;<i class="fa fa-times pull-right" aria-hidden="true"></i></a>
    					{% endif %}
						{% set m=m+1 %}
    				{% endfor %}
                {% endif %}
			{% endfor %}
		{% endif %}
		{% if options or attributes or prices_max_value %}{% else %}
			<span class="clearfix">
			{% if prices_max_value %}{{ text_option_no_empty }}{% endif %}
			</span>
		{% endif %}
		{% if ratings %}
			{% for rating in ratings %}
				{% if rating.rating_value_required %}
					<a class="btn btn-white option_cloud{{ rating.rating }}" onclick="{% if ratings_type == 'radio' %}live_option_product('rating_filter', 'options', '', '');{% else %}live_home_clear_rating_filter({{ rating.rating }});{% endif %}">
					<span class="rating">
						{% for i in 1..5 %}
							{% if rating.rating < i %}
								<span class="fa fa-stack"><i class="fa fa-star-o fa-stack-1x"></i></span>
							{% else %}
								<span class="fa fa-stack"><i class="fa fa-star fa-stack-1x"></i><i class="fa fa-star-o fa-stack-1x"></i></span>
							{% endif %}
						{% endfor %}
					</span>
					&nbsp;&nbsp;<i class="fa fa-times pull-right"></i></a>
				{% endif %}
			{% endfor %}
		{% endif %}
	</div>
	</div>
	{% for value in type_filters %}
		{% if view_categories %}
			{% if value == "categories" %}
				{% if status_category %}
						<div class="checkbox-group btn-border col-sm-12{% if view_categories == "column" %} column_filt{% if go_mobile %} column_mobile{% endif %}{% endif %}{% if view_categories == "content" %} content_filt{% endif %}{% if collapse_category == '1' %} collapse_filter{% endif %}{% if mobile_category == 0 %} hidden-xs{% endif %}{% if desktop_category == 0 %} hidden-sm{% endif %}">
							<div class="row"><h4 class="col-sm-12" onclick="$(this).parent().parent().find('.checkbox-group-overflow').slideToggle(300); $(this).parent().parent().find('.select-group').slideToggle(300); $(this).parent().parent().find('.text_more_group').slideToggle(300);"><strong>{{ text_category }}</strong><i class="fa fa-caret-down pull-left"></i></h4></div>
							{% set count = 0 %}
							{% set coun_end = 0 %}
							{% set count_group = 900 %}
							{% set test_checked = 0 %}{% if categories %}{% for category in categories %}{% if category.category_value_required %}{% set test_checked = test_checked + 1 %}{% endif %}{% endfor %}{% endif %}
							{% if category_type == 'link' %}
								<div class="checkbox-group-overflow categories_filter">
									{% if parent_category_id %}
										<div class="radio">
											<input type="hidden" name="parent_category_hierarchy" value="{{ parent_category_hierarchy.category_id }}" checked id="parent_category_hierarchy" />
											{% if old_category_id %}
												<input type="hidden" name="category_id" value="{{ category_id }}" checked id="common_category" />
												<input type="radio" name="category_id" value="{{ old_category_id }}" id="option-value-category-{{ old_category_id }}" />
													<i class="fa fa-reply-all pull-left"></i>
													<label for="option-value-category-{{ old_category_id }}">
														<a class="category_filter">{{ parent_category_hierarchy.name }}</a>
													</label>
											{% else %}
												<input type="hidden" name="category_id" value="{{ category_id }}" checked id="common_category" />
												<input type="hidden" name="old_category_id" value="{{ parent_category_hierarchy.category_id }}" id="" />
													<i class="fa fa-reply-all pull-left"></i>
													<label for="option-value-category-{{ parent_category_hierarchy.category_id }}">
														<a class="category_filter" onclick="live_option_product('category_filter', 'options', '', 'no_popup');">{{ parent_category_hierarchy.name }}</a>
													</label>
											{% endif %}
										</div>
									{% endif %}
									{% if categories %}
										{% for category in categories %}
											{% set count = count + 1 %}
											{% if category.category_stock_required or parent_category_id %}
												<div class="radio{% if category.category_stock_required == false %} not-active-link{% endif %}{% if test_checked == 0 %}{% if count > count_show %} none none{{ count_group }}{% endif %}{% endif %}">
													<input type="radio"{% if category.category_value_required %} checked{% endif %} name="category_id" value="{{ category.category_id }}" data-category="category-{{ category.category_id }}" id="option-value-category-{{ category.category_id }}"{% if category.category_stock_required == false %} disabled{% endif %} class="filter_hide" />
														{% if category.category_value_required %}<i class="fa fa-check pull-left"></i>{% endif %}
														<label for="option-value-category-{{ category.category_id }}">
															<a class="category_filter">{{ category.name }}</a>
															{% if category.category_value_required %}
																<i class="fa fa-close pull-right" onclick="live_home_filter();"></i>
															{% else %}
																{% if category.total and quantity_view %}<span class="filter_total">{{ category.total }}</span>{% else %}<span class="filter_total filter_gray">0</span>{% endif %}
															{% endif %}
														</label>
												</div>
												{% set coun_end = coun_end + 1 %}
											{% endif %}
										{% endfor %}
									{% endif %}
									<div class="hide"><span id="popup_categories"></span></div>
								</div>
								{% set coun_end = coun_end - count_show %}
								{% if test_checked == 0 and coun_end > 0 %}{% if count > count_show %}<div class="text_more_group"><a id="show_parent{{ count_group }}" onclick="show_parent({{ count_group }})"><strong class="text_more">{{ text_more }} ({{ coun_end }})</strong><strong class="text_hide none">{{ text_hide }} ({{ coun_end }})</strong></a></div>{% endif %}{% endif %}
							{% endif %}
							{% if category_type == 'select' %}
								{% if parent_category_id %}
									<div class="radio select_filter">
										<input type="hidden" name="parent_category_hierarchy" value="{{ parent_category_hierarchy }}" checked id="parent_category_hierarchy" />
										{% if old_category_id %}
											<a class="category_filter">
												<i class="fa fa-reply-all pull-left"></i>
												<input type="hidden" name="category_id" value="{{ category_id }}" checked id="common_category" />
												<input type="radio" name="category_id" value="{{ old_category_id }}" id="option-value-category-{{ old_category_id }}" />
												<label for="option-value-category-{{ old_category_id }}">
													{{ parent_category_hierarchy.name }}
												</label>
											</a>
										{% else %}
											<input type="hidden" name="category_id" value="{{ category_id }}" checked id="common_category" />
											<a class="category_filter" onclick="live_option_product('category_filter', 'options', '', 'no_popup');">
												<i class="fa fa-reply-all pull-left"></i>
												<input type="hidden" name="old_category_id" value="{{ parent_category_hierarchy.category_id }}" id="" />
												<label for="option-value-category-{{ parent_category_hierarchy.category_id }}">
													{{ parent_category_hierarchy.name }}
												</label>
											</a>
										{% endif %}
									</div>
								{% endif %}
								<div class="select-group">
									<select name="category_id" id="category-option" class="form-control">
										<option value="" id="common_category">{{ text_select }}</option>
										{% for category in categories %}
											{% if category.category_stock_required %}
											<option value="{{ category.category_id }}" {% if category.category_stock_required == false %} disabled{% endif %} {% if category.category_value_required %} selected{% endif %}>{{ category.name }}{% if category.total and quantity_view %} ({{ category.total }}){% else %} (0){% endif %}</option>
											{% endif %}
										{% endfor %}
									</select>
									<div><span id="popup_categories"></span></div>
								</div>
							{% endif %}
							{% if category_type == 'radio' %}
								<div class="checkbox-group-overflow categories_filter">
									{% if parent_category_id %}
										<div class="radio">
											<input type="hidden" name="parent_category_hierarchy" value="{{ parent_category_hierarchy }}" checked id="parent_category_hierarchy" />
											{%  if old_category_id %}
												<a class="category_filter">
													<i class="fa fa-reply-all pull-left"></i>
													<input type="hidden" name="category_id" value="{{ category_id }}" checked id="common_category" />
													<input type="radio" name="category_id" value="{{ old_category_id }}" id="option-value-category-{{ old_category_id }}" />
													
													<label for="option-value-category-{{ old_category_id }}">
														{{ parent_category_hierarchy.name }}
													</label>
												</a>
											{% else %}
												<input type="hidden" name="category_id" value="{{ category_id }}" checked id="common_category" />
												<a class="category_filter" onclick="live_option_product('category_filter', 'options', '', 'no_popup');">
													<i class="fa fa-reply-all pull-left"></i>
													<input type="hidden" name="old_category_id" value="{{ parent_category_hierarchy.category_id }}" id="" />
													<label for="option-value-category-{{ parent_category_hierarchy.category_id }}">
														{{ parent_category_hierarchy.name }}
													</label>
												</a>
											{% endif %}
										</div>
									{% endif %}
									{% for category in categories %}
										{% set count = count + 1 %}
										{% if category.category_stock_required or parent_category_id %}
											<div class="radio{% if test_checked == 0 %}{% if count > count_show %} none none{{ count_group }}{% endif %}{% endif %}">
											  <input type="radio"{% if category.category_value_required %} checked{% endif %} name="category_id" value="{{ category.category_id }}" data-category="category-{{ category.category_id }}" id="option-value-category-{{ category.category_id }}"{% if category.category_stock_required == false %} disabled{% endif %} />
											  <label for="option-value-category-{{ category.category_id }}">
												{{ category.name }}
												{% if category.category_value_required %}
													<i class="fa fa-close" onclick="live_option_product('category_filter', 'options', '', '');"></i>
												{% else %}
													{% if category.total and quantity_view %}<span class="filter_total">{{ category.total }}</span>{% else %}<span class="filter_total filter_gray">0</span>{% endif %}
												{% endif %}
											  </label>
											</div>
											{% set coun_end = coun_end + 1 %}
										{% endif %}
									{% endfor %}
									<div class="hide"><span id="popup_categories"></span></div>
								</div>
								{% set coun_end = coun_end - count_show %}
								{% if test_checked == 0 and coun_end > 0 %}{% if count > count_show %}<div class="text_more_group"><a id="show_parent{{ count_group }}" onclick="show_parent({{ count_group }})"><strong class="text_more">{{ text_more }} ({{ coun_end }})</strong><strong class="text_hide none">{{ text_hide }} ({{ coun_end }})</strong></a></div>{% endif %}{% endif %}
							{% endif %}
						</div>
					
				{% endif %}
			{% endif %}
		{% endif %}
		{% if max_price_const or old_price %}
			{% if view_prices %}
				{% if value == "prices" %}
					{% if status_price %}
						<div class="checkbox-group-price btn-border col-sm-12{% if view_prices == "column" %} column_filt{% if go_mobile %} column_mobile{% endif %}{% endif %}{% if view_prices == "content" %} content_filt{% endif %}{% if collapse_price == '1' %} collapse_filter{% endif %}{% if mobile_price == 0 %} hidden-xs{% endif %}{% if desktop_price == 0 %} hidden-sm{% endif %}">
							<div class="checkbox-group-overflow-price{% if view_prices == "column" %} column_filt{% endif %}{% if view_prices == "content" %} content_filt{% endif %}">
								<div class="row"><h4 class="col-sm-12" onclick="$(this).parent().parent().find('.formCost.checkbox').slideToggle(300); $(this).parent().parent().find('.text_more_group').slideToggle(300);"><strong>{{ text_range_price }}</strong><i class="fa fa-caret-down pull-left"></i></h4></div>
								<div class="formCost checkbox">
								{% if value_max_currency_left %}<label class="pull-left">{% if value_max_currency_left %}{{ value_max_currency_left }}{% endif %}</label>{% endif %}<input type="text" name="prices_min_value" id="minCost" value="{% if min_price %}{{ min_price }}{% else %}0{% endif %}" class="pull-left form-control" /><label class="pull-left">{% if value_max_currency_right %}{{ value_max_currency_right }}{% endif %}&nbsp;&nbsp;&nbsp;-&nbsp;&nbsp;</label>
								{% if value_max_currency_left %}<label class="pull-left">{% if value_max_currency_left %}{{ value_max_currency_left }}{% endif %}</label>{% endif %}<input type="text" name="prices_max_value" id="maxCost" value="{% if max_price_const %}{% if get_price_max > max_price_const %}{% if max_price_const %}{{max_price_const }}{% endif %}{% else %}{% if max_price %}{{ max_price }}{% endif %}{% endif %}{% else %}{{ prices_max_value }}{% endif %}" class="pull-left form-control" /> {% if value_max_currency_right %}<label for="maxCost" class="pull-left">{{ value_max_currency_right }}</label>{% endif %}
								<div class="clearfix"><div id="formCost_popup"></div></div>
								<div class="clearfix"></div><br />
								<div class="col-sm-12"><div class="slider"></div></div>
								</div><div class="clearfix"></div>
							</div>
						</div>
					{% endif %}
				{% endif %}
			{% endif %}
		{% endif %}
		{% if view_keywords_filter %}
			{% if value == "keywords_filter" %}
				{% if status_keywords %}
					{% if max_price_const %}
						<div class="checkbox-group btn-border col-sm-12{% if view_keywords_filter == "column" %} column_filt{% endif %}{% if view_keywords_filter == "content" %} content_filt{% endif %}{% if collapse_keywords == '1' %} collapse_filter{% endif %}{% if mobile_keywords == 0 %} hidden-xs{% endif %}{% if desktop_keywords == 0 %} hidden-sm{% endif %}">
							<div class="row"><h4 class="col-sm-12" onclick="$(this).parent().parent().find('.select-group').slideToggle(300); $(this).parent().parent().find('.text_more_group').slideToggle(300);"><strong>{{ text_keywords }}</strong><i class="fa fa-caret-down pull-right"></i></h4></div>
							<div class="select-group">
								<div id="keywords_filter" class="input-group select-group">
								  <input type="text" id="keywords_filter_input" name="keywords_filter" value="{% if keywords_filter_text %}{{ keywords_filter_text }}{% endif %}" placeholder="{% if delimitier == " " %}{{ text_keywords_placeholder }}{% endif %}{% if delimitier == "," %}{{ text_keywords_placeholder_zap }}{% endif %}" class="form-control" />
								  <span class="input-group-btn">
									<button type="button" class="btn"><i class="fa fa-search"></i></button>
								  </span>
								</div>
							</div>
						</div>
					{% endif %}
				{% endif %}
			{% endif %}
		{% endif %}
		{% if view_manufacturers %}
			{% if value == "manufacturers" %}
				{% if status_manufacturers %}
					{% if manufacturers %}
						<div class="checkbox-group btn-border col-sm-12{% if view_manufacturers == "column" %} column_filt{% endif %}{% if view_manufacturers == "content" %} content_filt{% endif %}{% if collapse_manufacturers == '1' %} collapse_filter{% endif %}{% if mobile_manufacturers == 0 %} hidden-xs{% endif %}{% if desktop_manufacturers == 0 %} hidden-sm{% endif %}">
							<div class="row"><h4 class="col-sm-12" onclick="$(this).parent().parent().find('.checkbox-group-overflow').slideToggle(300); $(this).parent().parent().find('.select-group').slideToggle(300); $(this).parent().parent().find('.text_more_group').slideToggle(300);"><strong>{{ text_manufacturers }}</strong><i class="fa fa-caret-down pull-left"></i></h4></div>
							{% set count = 0 %}
							{% set coun_end = 0 %}
							{% set count_group = 100 %}
							{% if manufacturers_type == 'checkbox' %}
								<div class="checkbox-group-overflow">
								{% set test_checked = 0 %}{% if manufacturers %}{% for manufacturer in manufacturers %}{% if manufacturer.manufacturer_value_required %}{% set test_checked = test_checked + 1 %}{% endif %}{% endfor %}{% endif %}
									{% for manufacturer in manufacturers %}
										{% set count = count + 1 %}
										<div class="checkbox{% if test_checked == 0 %}{% if count > count_show %} none none{{ count_group }}{% endif %}{% endif %}">
										  <input type="checkbox" name="manufacturers_filter[]" value="{{ manufacturer.manufacturer_id }}"{% if manufacturer.manufacturer_value_required %} checked{% endif %} data-manufacturers="manufacturers-{{ manufacturer.manufacturer_id }}" id="option-value-manufacturers-{{ manufacturer.manufacturer_id }}"{% if manufacturer.manufacturers_stock_required == false and manufacturer.manufacturer_value_required == false and manufacturer.manufacturer_value_required == false %} disabled{% endif %} />
										  <label for="option-value-manufacturers-{{ manufacturer.manufacturer_id }}">
											{% if manufacturer.image %}<img src="{{ manufacturer.image }}" alt="" title="" />{% endif %}
											{{ manufacturer.name }}
											{% if manufacturer.manufacturer_value_required %}
												{% if manufacturer.manufacturers_stock_required %}
													<i class="fa fa-close pull-right"></i>
												{% else %}
													<span class="filter_total filter_gray showing">0</span>
												{% endif %}
											{% else %}
